  - coingecko
  - DAI
  - USD
datasource:
  url: https://api.coingecko.com/api/v3/simple/price
  headers:
    Content-Type: application/json
  queries:
    ids: dai
    vs_currencies: usd
storage:
  with_timestamp: true
interval: 3600

